{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec npm run babel-node --  ./src/server.js",
    "babel-node": "babel-node",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "generate-models": "bash -c 'sequelize-auto -o \"./src/models\" -d $0 -h localhost -u $1 -p 3306 -x $2 -e mysql --skipTables sequelizemeta --noInitModels'",
    "addRoute": "node ./cli/addRoute/main.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.js",
      "**/*.spec.js",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "chalk": "^4.1.1",
    "compose-middleware": "^5.0.1",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-swagger-generator": "^1.1.17",
    "faker": "^5.5.3",
    "got": "^11.8.2",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "pdfkit": "^0.12.1",
    "prompts": "^2.4.1",
    "rotating-file-stream": "^2.1.5",
    "sequelize": "^6.6.5",
    "sequelize-auto": "^0.8.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.6",
    "regenerator-runtime": "^0.13.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.4"
  }
}
