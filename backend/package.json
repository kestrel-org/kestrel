{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec npm run babel-node --  ./src/server.js",
    "babel-node": "babel-node",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "generate-models": "bash -c 'sequelize-auto -o \"./src/models\" -d $0 -h localhost -u $1 -p 3306 -x $2 -e mysql --skipTables sequelizemeta'"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.js",
      "**/*.spec.js",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.0",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-swagger-generator": "^1.1.17",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "pdfkit": "^0.12.1",
    "sequelize": "^6.6.2",
    "sequelize-auto": "^0.8.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "regenerator-runtime": "^0.13.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
